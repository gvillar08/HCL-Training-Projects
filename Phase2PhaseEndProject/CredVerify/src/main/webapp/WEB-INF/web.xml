<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>CredVerify</display-name>
  <welcome-file-list>
    <welcome-file>main.jsp</welcome-file>
  </welcome-file-list>
  <servlet>											<!-- For creating servlets and mapping them. Nothing fun going on here -->
    <description></description>
    <display-name>UserRegister</display-name>
    <servlet-name>UserRegister</servlet-name>
    <servlet-class>UserRegister</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>UserAuthenticate</display-name>
    <servlet-name>UserAuthenticate</servlet-name>
    <servlet-class>UserAuthenticate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserRegister</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserAuthenticate</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>